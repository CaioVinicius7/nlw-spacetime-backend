{
  "extends": ["@rocketseat/eslint-config/node"],
  "plugins": ["prettier"],
  "rules": {
    "no-useless-constructor": "off",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": false,
        "arrowParens": "always",
        "semi": true,
        "endOfLine": "auto",
        "trailingComma": "none"
      }
    ]
  }
}
